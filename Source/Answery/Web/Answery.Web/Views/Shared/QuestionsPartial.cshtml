@model IEnumerable<Answery.Web.ViewModels.Question.QuestionViewModel>

<div class="row special-style">
    <div class="col 12">
        <div class="card-panel grey darken-4">
            <h2 class="center white-text text-darken-4 flow-text">Answered Questions</h2>
        </div>
    </div>
</div>

<div>
    @if (Model.Count() == 0)
    {
        <div class="row">
            <div class="col s12">
                <div class="card-panel">
                    <h2 class="center blue-text text-darken-2 flow-text">You haven't answered any questions yet :(</h2>
                </div>
            </div>
        </div>

    }
    else
    {
        foreach (var question in Model)
        {
         <div>question.Content</div>
       }
    }
</div>