@model IEnumerable<Answery.Web.ViewModels.Question.QuestionViewModel>

<h2 class="center">Answered Questions</h2>

<div>
    @if (Model.Count() == 0)
    {
        <div>You don't have any questions yet :(</div>
    }
    else
    {
       foreach (var question in Model)
       {
         <div>question.Content</div>
       }
    }
</div>